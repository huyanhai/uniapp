<block wx:if="{{show}}"><view class="stores-content"><view class="top"><image class="post" src="{{item.headImg}}" data-event-opts="{{[['tap',[['goShopDetails',['$0'],['item.stringId']]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['goShopDetails',['$0'],['item.stringId']]]]]}}" class="info" bindtap="__e"><view class="name">{{item.shopName}}</view><view class="address">{{"地址："+(item.address||'')}}</view><view class="time">{{"营业时间："+(item.startTime+'-'+item.endTime)}}</view></view><view data-event-opts="{{[['tap',[['goMap',['$event']]]]]}}" class="go icon-go" bindtap="__e"></view></view><view class="bottom"><view class="data-item">可租借：<text class="num">{{item.canBorrowQuantity}}</text></view><view class="data-item">可归还：<text class="num">{{item.returnableQuantity}}</text></view><view class="data-item">距离：<text class="num">{{$root.m0+"米"}}</text></view></view></view></block>