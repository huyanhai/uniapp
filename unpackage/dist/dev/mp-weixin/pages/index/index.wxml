<view class="content" disable-scroll="true"><block wx:if="{{!showSheet}}"><view class="user-layer"><image class="user-post" src="{{userInfo.headUrl}}" data-event-opts="{{[['tap',[['goUser',['$event']]]]]}}" bindtap="__e"></image><view class="text">欢迎使用起电！</view><button data-event-opts="{{[['tap',[['goNearShop',['$event']]]]]}}" class="shop" bindtap="__e"><view class="icon icon-shop"></view>附件商家</button></view></block><block wx:if="{{orders.length>0}}"><view class="task"><icon class="battery icon-battery"></icon><view class="info"><view class="top">{{orders.length+"个订单租借中……"}}</view><view class="bottom">可归还至其他商家</view></view><button data-event-opts="{{[['tap',[['goShopDetail',['$0'],['orders.__$n0']]]]]}}" class="more" bindtap="__e">查看详情</button></view></block><view data-event-opts="{{[['tap',[['scanCode',['$event']]]]]}}" class="option-scan" bindtap="__e"><view class="icon icon-scan"></view>扫一扫借用</view><view class="right-option"><view data-event-opts="{{[['tap',[['goLocation',['$event']]]]]}}" class="location" bindtap="__e"><view class="icon-location"></view></view><view data-event-opts="{{[['tap',[['callMe',['$event']]]]]}}" class="call-me" bindtap="__e"><view class="icon icon-call"></view></view></view><login-sheet vue-id="8dd740cc-1" show="{{showSheet}}" data-event-opts="{{[['^authSuccess',[['authSuccess']]]]}}" bind:authSuccess="__e" bind:__l="__l"></login-sheet><map class="map" style="height:100vh;" id="map" longitude="{{addressInfo.longitude}}" latitude="{{addressInfo.latitude}}" scale="{{scale}}" show-location="true" markers="{{markers}}" data-event-opts="{{[['regionchange',[['regionchange',['$event']]]],['markertap',[['makertap',['$event']]]],['tap',[['hideShop',['$event']]]]]}}" bindregionchange="__e" bindmarkertap="__e" bindtap="__e"></map><call-us vue-id="8dd740cc-2" show="{{showCallUs}}" data-event-opts="{{[['^updateShow',[['__set_sync',['$0','showCallUs','$event'],['']]]]]}}" bind:updateShow="__e" bind:__l="__l"></call-us><cover-image class="map-center" src="http://wd-qidian.oss-cn-beijing.aliyuncs.com/mini/icon-lo.png"></cover-image><view class="store-box"><shop-item vue-id="8dd740cc-3" show="{{showShopItem}}" item="{{shopItemData}}" latitude="{{addressInfo.latitude}}" longitude="{{addressInfo.longitude}}" data-event-opts="{{[['^updateLatitude',[['__set_sync',['$0','latitude','$event'],['addressInfo']]]],['^updateLongitude',[['__set_sync',['$0','longitude','$event'],['addressInfo']]]]]}}" bind:updateLatitude="__e" bind:updateLongitude="__e" bind:__l="__l"></shop-item></view></view>