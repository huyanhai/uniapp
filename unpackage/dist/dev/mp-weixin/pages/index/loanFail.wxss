.loan-success-content {
  padding: 30rpx;
  box-sizing: border-box;
}
.loan-success-content .studus-img {
    width: 428rpx;
    height: 364rpx;
    margin: 60rpx auto 40rpx auto;
    display: block;
}
.loan-success-content .tips {
    font-size: 28rpx;
    color: #333333;
    text-align: center;
    display: block;
}
.loan-success-content .info {
    font-size: 26rpx;
    color: #999999;
    text-align: center;
    display: block;
    margin: 30rpx 0 250rpx 0;
}
.loan-success-content .info .num {
      color: #22A6F1;
}
.loan-success-content .back-home {
    height: 90rpx;
    margin: 70rpx 0 0 0;
    background: #22A6F1;
    color: #FFFFFF;
    border-radius: 90rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
}
.loan-success-content .call {
    height: 90rpx;
    margin: 40rpx 0 0 0;
    color: #22A6F1;
    border: 2rpx solid #22A6F1;
    border-radius: 90rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
}

