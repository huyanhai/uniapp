<view class="near-shop"><view class="searchs"><image class="img" src="http://wd-qidian.oss-cn-beijing.aliyuncs.com/mini/search.png" mode></image><input type="text" placeholder="搜索附近充电宝网点" data-event-opts="{{[['input',[['__set_model',['','shopName','$event',[]]]]]]}}" value="{{shopName}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getNearData',['$event']]]]]}}" class="btn" bindtap="__e">搜索</view></view><block wx:if="{{shopData.length<1}}"><text class="no-order">附近暂无商家</text></block><block wx:for="{{shopData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><shop-item vue-id="{{'5914c072-1-'+index}}" item="{{item.shopInfo}}" latitude="{{latitude}}" longitude="{{longitude}}" data-event-opts="{{[['^updateLatitude',[['__set_sync',['$0','latitude','$event'],['']]]],['^updateLongitude',[['__set_sync',['$0','longitude','$event'],['']]]]]}}" bind:updateLatitude="__e" bind:updateLongitude="__e" bind:__l="__l"></shop-item></block></view>