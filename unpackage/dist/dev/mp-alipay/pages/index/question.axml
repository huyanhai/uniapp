<view class="question-content"><input class="ui-input" placeholder="请输入设备编号"/><view class="category"><text class="label">分类标签</text><view class="item-box"><radio-group data-event-opts="{{[['change',[['changed',['$event']]]]]}}" class="group" onChange="__e"><block a:for="{{items}}" a:for-item="item" a:for-index="index" a:key="index"><label class="{{(('ui-radio')+' '+((item.checked)?'active':''))}}"><radio value="{{item.value}}" checked="{{item.checked}}"></radio><text class="text">{{item.name}}</text></label></block></radio-group></view></view><view class="upload-img"><text class="label">我要反馈</text><view class="form"><textarea class="textarea" placeholder="请输入问题或意见"></textarea><view class="lists"><view data-event-opts="{{[['tap',[['uploadImg',['$event']]]]]}}" class="upload" onTap="__e"></view><block a:for="{{imgList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="list"><icon data-event-opts="{{[['tap',[['delImg',[index]]]]]}}" class="close" onTap="__e"></icon><image class="img" src="{{item}}"></image></view></block></view></view></view><view class="call-us"><text class="label">联系方式</text><input class="ui-input" placeholder="请输入联系电话" type="number"/></view><view class="submit">提交</view></view>