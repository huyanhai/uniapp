<view class="near-shop"><block a:if="{{shopData.length<1}}"><text class="no-order">近期暂无商家</text></block><block a:for="{{shopData}}" a:for-item="item" a:for-index="index" a:key="index"><shop-item vue-id="{{'590ac048-1-'+index}}" item="{{item.shopInfo}}" latitude="{{latitude}}" longitude="{{longitude}}" data-event-opts="{{[['^updateLatitude',[['__set_sync',['$0','latitude','$event'],['']]]],['^updateLongitude',[['__set_sync',['$0','longitude','$event'],['']]]]]}}" onUpdateLatitude="__e" onUpdateLongitude="__e" onVueInit="__l"></shop-item></block></view>