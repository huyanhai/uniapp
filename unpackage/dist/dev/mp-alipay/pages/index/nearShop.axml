<view class="near-shop"><view class="searchs"><image class="img" src="http://wd-qidian.oss-cn-beijing.aliyuncs.com/mini/search.png" mode></image><input type="text" placeholder="搜索附近充电宝网点" data-event-opts="{{[['input',[['__set_model',['','shopName','$event',[]]]]]]}}" value="{{shopName}}" onInput="__e"/><view data-event-opts="{{[['tap',[['getNearData',['$event']]]]]}}" class="btn" onTap="__e">搜索</view></view><block a:if="{{shopData.length<1}}"><text class="no-order">附近暂无商家</text></block><shop-item vue-id="{{'590ac048-1-'+index}}" item="{{item.shopInfo}}" latitude="{{latitude}}" longitude="{{longitude}}" data-event-opts="{{[['^updateLatitude',[['__set_sync',['$0','latitude','$event'],['']]]],['^updateLongitude',[['__set_sync',['$0','longitude','$event'],['']]]]]}}" onUpdateLatitude="__e" onUpdateLongitude="__e" a:for="{{shopData}}" a:for-item="item" a:for-index="index" a:key="index" onVueInit="__l"></shop-item></view>