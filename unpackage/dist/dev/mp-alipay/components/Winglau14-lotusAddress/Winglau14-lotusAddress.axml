<block a:if="{{lotusAddressData.visible}}"><view class="lotus-address-mask" status="{{checkStatus}}"><view class="{{(lotusAddressData.visible?'lotus-address-box':'lotus-address-box lotus-address-box-out')}}"><view class="lotus-address-action"><text data-event-opts="{{[['tap',[['cancelPicker',['$event']]]]]}}" class="lotus-address-action-cancel" onTap="__e">取消</text><text data-event-opts="{{[['tap',[['chosedVal',['$event']]]]]}}" class="lotus-address-action-affirm" onTap="__e">确认</text></view><view class="lotus-address-picker-box"><scroll-view class="lotus-address-picker-box-item" scroll-y="{{true}}" scroll-into-view="{{'pid'+pChoseIndex}}"><view class="{{(pIndex===pChoseIndex?'lotus-address-picker lotus-address-picker2':'lotus-address-picker')}}" id="{{'pid'+pIndex}}" data-event-opts="{{[['tap',[['clickPicker',[0,pIndex,'$0'],[[['province','',pIndex]]]]]]]}}" onTap="__e" a:for="{{province}}" a:for-item="pItem" a:for-index="pIndex" a:key="pIndex">{{pItem}}</view></scroll-view><scroll-view class="lotus-address-picker-box-item" scroll-y="{{true}}" scroll-into-view="{{'cid'+cChoseIndex}}"><view class="{{(cIndex===cChoseIndex?'lotus-address-picker lotus-address-picker2':'lotus-address-picker')}}" id="{{'cid'+cIndex}}" data-event-opts="{{[['tap',[['clickPicker',[1,cIndex,'$0'],[[['city','',cIndex]]]]]]]}}" onTap="__e" a:for="{{city}}" a:for-item="cItem" a:for-index="cIndex" a:key="cIndex">{{cItem}}</view></scroll-view><scroll-view class="lotus-address-picker-box-item" scroll-y="{{true}}" scroll-into-view="{{'tid'+tChoseIndex}}"><view class="{{(tIndex===tChoseIndex?'lotus-address-picker lotus-address-picker2':'lotus-address-picker')}}" id="{{'tid'+tIndex}}" data-event-opts="{{[['tap',[['clickPicker',[2,tIndex,'$0'],[[['town','',tIndex]]]]]]]}}" onTap="__e" a:for="{{town}}" a:for-item="tItem" a:for-index="tIndex" a:key="tIndex">{{tItem}}</view></scroll-view></view></view></view></block>