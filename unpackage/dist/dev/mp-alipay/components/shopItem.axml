<block a:if="{{show}}"><view class="stores-content"><view class="top"><image class="post" src="{{item.headImg}}" data-event-opts="{{[['tap',[['goShopDetails',['$0'],['item.stringId']]]]]}}" onTap="__e"></image><view data-event-opts="{{[['tap',[['goShopDetails',['$0'],['item.stringId']]]]]}}" class="info" onTap="__e"><view class="name">{{item.shopName}}</view><view class="address">{{"地址："+(item.address||'')}}</view><view class="time">{{"营业时间："+((item.startTime||'')+'-'+(item.endTime||''))}}</view></view><text class="dis-num">{{$root.m0+"米"}}</text></view><view class="bottom"><view class="data-item">可租借：<text class="num">{{item.canBorrowQuantity}}</text></view><view class="data-item">可归还：<text class="num">{{item.returnableQuantity}}</text></view><view class="data-item"><view data-event-opts="{{[['tap',[['goMap',['$event']]]]]}}" class="gos" onTap="__e"><view class="go icon-go"></view>到这去</view></view></view></view></block>