<view data-event-opts="{{[['tap',[['goDetail',['$0'],['item']]]]]}}" class="order-item-content" onTap="__e"><view class="top"><view class="stadus error warning ok">{{orderStatus[item.leaseStatus]||'无状态'}}</view></view><view class="info"><view class="item"><text class="name">订单编号：</text><text class="detail num">{{''+item.stringId+''}}</text></view><view class="item"><text class="name">租借费用：</text><block a:if="{{item.totalMoney&&item.totalMoney!=='null'||item.totalMoney===0}}"><text class="detail num">{{''+item.totalMoney+'元'}}</text></block></view><view class="item"><text class="name">设备商家：</text><text class="detail">{{item.shopName}}</text></view><view class="item"><text class="name">租借时间：</text><text class="detail">{{item.updateDate}}</text></view></view><view class="pro-name">充电宝</view></view>